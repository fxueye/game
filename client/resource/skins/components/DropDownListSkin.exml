		<e:Skin states="up,down,disabled">
			<e:Image width="119" height="58" source="common_btn_px_png" source.down="common_btn_px_png" source.disabled="common_btn_px_png"/>
			<e:Label id="labelDisplay" horizontalCenter="0" verticalCenter="0" textColor="0x35b0f8" fontFamily="SimHei" size="20" bold="true"/>
		</e:Skin>
		</e:skinName>
