<?xml version="1.0" encoding="utf-8"?>
<e:Skin class="skin.DropDownListSkin" width="106" minHeight="141" xmlns:e="http://ns.egret.com/eui" xmlns:w="http://ns.egret.com/wing">
	<e:Group id="gpList" width="106" height="141" y="0" horizontalCenter="0">
		<e:Image width="100%" height="100%" source="common_bg_pxd_png"  scale9Grid="3,3,18,18"></e:Image>
		<e:Scroller id="scroller" width="100%" height="100%" y="0" horizontalCenter="0">
			<e:List id="ddlList" anchorOffsetX="0" width="100">
